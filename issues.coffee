issues_numbers = []
api_issue_source = 'https://api.github.com/repos/communityNTNU/Data-for-watchNTNU/issues?'
$.getJSON api_issue_source, (issues) ->
  $.each issues, (k, d) ->

    ###
        Here are the useful shit:
        d.html_url
        d.state
        d.title
        d.number
        d.body
        d.user.login
        d.user.html_url
        d.locked
        d.comments
        d.closed_at
        d.created_at
        d.update_at
    ###

    issues_numbers.push d.number
